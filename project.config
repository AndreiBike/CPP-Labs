[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/AndreiBike
	label-Code-Review = -2..+2 group P****i
[access "refs/for/refs/*"]
	read = group user/AndreiBike
	push = group user/AndreiBike
	label-Code-Review = -2..+2 group user/AndreiBike
	label-Verified = -1..+1 group user/AndreiBike
	rebase = group user/AndreiBike
	removeReviewer = group user/AndreiBike
	submit = group user/AndreiBike
[access "refs/heads/*"]
	read = group user/AndreiBike
	create = group user/AndreiBike
	push = +force group user/AndreiBike
	pushMerge = group user/AndreiBike
[access "refs/tags/*"]
	push = +force group user/AndreiBike
	createTag = group user/AndreiBike
	createSignedTag = group user/AndreiBike
